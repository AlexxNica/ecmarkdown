{
  "name": "ecmarkdown",
  "version": "1.1.0",
  "description": "A compiler for \"Ecmarkdown\" algorithm shorthand into HTML.",
  "main": "lib/ecmarkdown.js",
  "scripts": {
    "prepublish": "node scripts/generate-grammar.js < lib/grammar.pegjs > lib/generated-parser.js",
    "pretest": "npm run prepublish",
    "test": "node test/run-cases.js",
    "lint": "jshint lib && jscs lib && jshint test && jscs test"
  },
  "repository": "https://github.com/domenic/ecmarkdown.git",
  "keywords": [
    "ecmascript",
    "javascript",
    "specs",
    "typography",
    "markup",
    "markdown",
    "html",
    "code"
  ],
  "files": [
    "lib/*.js",
    "LICENSE.txt"
  ],
  "dependencies": {
    "cheerio": "^0.17.0",
    "escape-html": "^1.0.1",
    "inline-elements": "^0.1.1",
    "js-beautify": "^1.5.4",
    "pegjs": "^0.8.0"
  },
  "devDependencies": {
    "baseline-tester": "^1.1.1",
    "bluebird": "^2.6.2",
    "glob": "^4.0.5",
    "jscs": "^1.6.1",
    "jshint": "^2.5.5"
  },
  "author": "Domenic Denicola <domenic@domenicdenicola.com> (http://domenic.me/)",
  "license": "WTFPL"
}
